<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Tishko.App"
             xmlns:local="using:Tishko"
             RequestedThemeVariant="Default">

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  <Application.Resources>
      <ResourceDictionary>
          <ResourceDictionary.MergedDictionaries>
              <MergeResourceInclude Source="Styles/Colors.axaml"></MergeResourceInclude>
          </ResourceDictionary.MergedDictionaries>
      </ResourceDictionary>
  </Application.Resources>
    <Application.Styles>
        <StyleInclude Source="Styles/IconButton.axaml"></StyleInclude>
    </Application.Styles>
</Application>